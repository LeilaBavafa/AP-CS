package dogs;

public class Dog {
	public String name;
	private String color;
	public Dog(String name) {
		// put necessary code here
		this.name = name;
		color = "";
	}

	public String getName() {
		// put necessary code here
		return name;
	}

	public String speak() {
		// put necessary code here
		return "woof";
	}
	public void setColor(String color) {
		this.color = color;
	}
	public String getColor() {
		return color;
	}
	public String toString() {
		return name + " is a " + getClass().toString() + "and says" + speak();
	}
}
